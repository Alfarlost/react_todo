{
  "name": "react-todo",
  "version": "1.0.0",
  "description": "Simple react todo list",
  "main": "src/server.js",
  "repository": "git@github.com:Alfarlost/react-todo.git",
  "scripts": {
    "start": "NODE_ENV=production node_modules/.bin/babel-node --presets 'react,es2015' src/server.js",
    "start-dev": "npm run start-dev-hmr",
    "start-dev-single-page": "node_modules/.bin/http-server src/static",
    "start-dev-hmr": "node_modules/.bin/webpack-dev-server --progress --inline --hot",
    "build": "NODE_ENV=production node_modules/.bin/webpack -p"
  },
  "author": "Vito Zhiliky",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-react-html-attrs": "^2.0.0",
    "babel-polyfill": "*",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "*",
    "react-router": "^4.1.2",
    "redux": "^3.7.2",
    "classnames": "*"
  },
  "devDependencies": {
    "bootstrap-loader": "*",
    "bootstrap-sass": "^3.3.7",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "*",
    "http-server": "^0.10.0",
    "node-sass": "^4.5.3",
    "react-hot-loader": "^1.3.1",
    "resolve-url-loader": "*",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "url-loader": "*",
    "webpack": "^3.4.1",
    "webpack-dev-server": "^2.6.1"
  }
